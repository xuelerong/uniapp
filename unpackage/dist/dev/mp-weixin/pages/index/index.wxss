}
page .content {
  padding-top: 150rpx;
  height: 100%;
  overflow: hidden;
}
page .content .login_text {
  position: fixed;
  color: #ed7830;
  font-weight: 600;
  font-family: 华文行楷;
  font-size: 60rpx;
  left: 60rpx;
}
